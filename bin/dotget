#!/bin/bash

cp -u -v $HOME/.config/i3/config $HOME/dotfiles/.config/i3/config
cp -u -v $HOME/.config/polybar/config $HOME/dotfiles/.config/polybar/config

cp -u -r -v $HOME/bin $HOME/dotfiles/bin
cp -u -v $HOME/.bashrc $HOME/dotfiles/.bashrc
cp -u -v $HOME/.fehbg $HOME/dotfiles/.fehbg

cp -u -v $HOME/.config/terminator/config $HOME/dotfiles/.config/terminator/config
